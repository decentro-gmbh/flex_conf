{
    "extends": "airbnb-base",
    "rules": {
        "strict": 0,
        "max-len": [2, {"code": 300, "tabWidth": 4, "ignoreUrls": true}],
        "no-param-reassign": [0],
        "dot-notation": 1,
        "comma-dangle": 1,
        "arrow-body-style": [0, "always"],
        "no-restricted-syntax": [0],
        "no-underscore-dangle": [0],
        "func-names": [0],
        "no-continue": [0],
        "no-multi-str": [0],
        "import/order": [0],
        "class-methods-use-this": [0],
        "no-unused-vars": [0]
    }
}
